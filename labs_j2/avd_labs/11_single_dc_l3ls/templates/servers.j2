hostname: {{ hostname }}
is_deployed: true
static_routes:
- vrf: MGMT
  destination_address_prefix: 0.0.0.0/0
  gateway: 172.16.2.1
- vrf: default
  destination_address_prefix: 10.1.0.0/16
  gateway: {{ static_route_default_gw }}
service_routing_protocols_model: multi-agent
vlan_internal_order:
  allocation: ascending
  range:
    beginning: 1006
    ending: 1199
ip_name_servers:
- ip_address: 1.1.1.1
  vrf: MGMT
- ip_address: 8.8.8.8
  vrf: MGMT
spanning_tree:
  mode: mstp
local_users:
- name: admin
  privilege: 15
  role: network-admin
  no_password: true
- name: ansible
  privilege: 15
  role: network-admin
  sha512_password: $6$7u4j1rkb3VELgcZE$EJt2Qff8kd/TapRoci0XaIZsL4tFzgq1YZBLD9c6f/knXzvcYY0NcMKndZeCv0T268knGKhOEwZAxqKjlMm920
vrfs:
- name: MGMT
  ip_routing: false
management_interfaces:
- name: Management1
  description: oob_management
  shutdown: false
  vrf: MGMT
  ip_address: {{ mgmt_ip_address }}
  gateway: 172.16.2.1
  type: oob
management_api_http:
  enable_vrfs:
  - name: MGMT
  enable_https: true
port_channel_interfaces:
- name: Port-Channel1
  description: {{ Po1_description }}
  type: routed
  shutdown: false
- name: Port-Channel1.{{ Po1_SubInt_vlan }}
  description: {{ Po1_SubInt_description }}
  type: l3dot1q
  shutdown: false
  encapsulation_dot1q_vlan: {{ Po1_SubInt_vlan }}
  ip_address: {{ Po1_SubInt_ip_address }}
ethernet_interfaces:
- name: Ethernet1
  description: {{ eth1_description }}
  type: port-channel-member
  shutdown: false
  channel_group:
    id: 1
    mode: active
- name: Ethernet2
  description: {{ eth2_description }}
  type: port-channel-member
  shutdown: false
  channel_group:
    id: 1
    mode: active
